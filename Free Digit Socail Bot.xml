<xml xmlns="https://developers.google.com/blockly/xml" is_dbot="true" collection="false">
  <variables>
    <variable id="Prediction">Prediction</variable>
    <variable id="Time">Time</variable>
    <variable id="Initial_Amount">Initial Amount</variable>
    <variable id="text">text</variable>
    <variable id="text1">text1</variable>
    <variable id="text2">text2</variable>
    <variable id="Digit">Digit</variable>
    <variable id="Stop_Loss">Stop Loss</variable>
    <variable id="Expected_Profit">Expected Profit</variable>
    <variable id="Win_Amount">Win Amount</variable>
    <variable id="text3">text3</variable>
    <variable id="FibCurrent">FibCurrent</variable>
    <variable id="FibPrev">FibPrev</variable>
    <variable id="BB_Upper">BB_Upper</variable>
    <variable id="BB_Lower">BB_Lower</variable>
    <variable id="RSI">RSI</variable>
  </variables>
  
  <block type="trade_definition" id="trade_def">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">1HZ100V</field>
        <next>
          <block type="trade_definition_tradetype">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">overunder</field>
            <next>
              <block type="trade_definition_contracttype">
                <field name="TYPE_LIST">both</field>
                <next>
                  <block type="trade_definition_candleinterval">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    <next>
                      <block type="trade_definition_restartbuysell">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        <next>
                          <block type="trade_definition_restartonerror">
                            <field name="RESTARTONERROR">FALSE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="INITIALIZATION">
      <block type="variables_set">
        <field name="VAR" id="Prediction">Prediction</field>
        <value name="VALUE">
          <block type="math_number">
            <field name="NUM">1</field>
          </block>
        </value>
        <next>
          <block type="variables_set">
            <field name="VAR" id="Time">Time</field>
            <value name="VALUE">
              <block type="math_number">
                <field name="NUM">1</field>
              </block>
            </value>
            <next>
              <block type="variables_set">
                <field name="VAR" id="Digit">Digit</field>
                <value name="VALUE">
                  <block type="math_number">
                    <field name="NUM">8</field>
                  </block>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR" id="Stop_Loss">Stop Loss</field>
                    <value name="VALUE">
                      <block type="math_number">
                        <field name="NUM">50</field>
                      </block>
                    </value>
                    <next>
                      <block type="variables_set">
                        <field name="VAR" id="Expected_Profit">Expected Profit</field>
                        <value name="VALUE">
                          <block type="math_number">
                            <field name="NUM">100</field>
                          </block>
                        </value>
                        <next>
                          <block type="variables_set">
                            <field name="VAR" id="Initial_Amount">Initial Amount</field>
                            <value name="VALUE">
                              <block type="math_number">
                                <field name="NUM">0.5</field>
                              </block>
                            </value>
                            <next>
                              <block type="variables_set">
                                <field name="VAR" id="Win_Amount">Win Amount</field>
                                <value name="VALUE">
                                  <block type="math_number">
                                    <field name="NUM">0.5</field>
                                  </block>
                                </value>
                                <next>
                                  <block type="variables_set">
                                    <field name="VAR" id="FibPrev">FibPrev</field>
                                    <value name="VALUE">
                                      <block type="math_number">
                                        <field name="NUM">0</field>
                                      </block>
                                    </value>
                                    <next>
                                      <block type="variables_set">
                                        <field name="VAR" id="FibCurrent">FibCurrent</field>
                                        <value name="VALUE">
                                          <block type="math_number">
                                            <field name="NUM">1</field>
                                          </block>
                                        </value>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="SUBMARKET">
      <block type="trade_definition_tradeoptions">
        <mutation has_prediction="true"></mutation>
        <field name="DURATIONTYPE_LIST">t</field>
        <value name="DURATION">
          <block type="variables_get">
            <field name="VAR" id="Time">Time</field>
          </block>
        </value>
        <value name="AMOUNT">
          <block type="variables_get">
            <field name="VAR" id="Initial_Amount">Initial Amount</field>
          </block>
        </value>
        <value name="PREDICTION">
          <block type="variables_get">
            <field name="VAR" id="Prediction">Prediction</field>
          </block>
        </value>
      </block>
    </statement>
  </block>
  
  <block type="after_purchase" id="after_purchase">
    <statement name="AFTERPURCHASE_STACK">
      <block type="controls_if">
        <mutation else="1"></mutation>
        <value name="IF0">
          <block type="contract_check_result">
            <field name="CHECK_RESULT">win</field>
          </block>
        </value>
        <statement name="DO0">
          <block type="text_join">
            <field name="VARIABLE" id="text">text</field>
            <statement name="STACK">
              <block type="text_statement">
                <value name="TEXT">
                  <block type="text">
                    <field name="TEXT">Won: </field>
                  </block>
                </value>
                <next>
                  <block type="text_statement">
                    <value name="TEXT">
                      <block type="read_details">
                        <field name="DETAIL_INDEX">4</field>
                      </block>
                    </value>
                  </block>
                </next>
              </block>
            </statement>
            <next>
              <block type="notify">
                <field name="NOTIFICATION_TYPE">success</field>
                <field name="NOTIFICATION_SOUND">silent</field>
                <value name="MESSAGE">
                  <block type="variables_get">
                    <field name="VAR" id="text">text</field>
                  </block>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR" id="Initial_Amount">Initial Amount</field>
                    <value name="VALUE">
                      <block type="variables_get">
                        <field name="VAR" id="Win_Amount">Win Amount</field>
                      </block>
                    </value>
                    <next>
                      <block type="variables_set">
                        <field name="VAR" id="Prediction">Prediction</field>
                        <value name="VALUE">
                          <block type="math_number">
                            <field name="NUM">3</field>
                          </block>
                        </value>
                        <next>
                          <block type="variables_set">
                            <field name="VAR" id="Digit">Digit</field>
                            <value name="VALUE">
                              <block type="math_number">
                                <field name="NUM">5</field>
                              </block>
                            </value>
                            <next>
                              <block type="variables_set">
                                <field name="VAR" id="Time">Time</field>
                                <value name="VALUE">
                                  <block type="math_number">
                                    <field name="NUM">3</field>
                                  </block>
                                </value>
                                <next>
                                  <block type="variables_set">
                                    <field name="VAR" id="FibPrev">FibPrev</field>
                                    <value name="VALUE">
                                      <block type="math_number">
                                        <field name="NUM">0</field>
                                      </block>
                                    </value>
                                    <next>
                                      <block type="variables_set">
                                        <field name="VAR" id="FibCurrent">FibCurrent</field>
                                        <value name="VALUE">
                                          <block type="math_number">
                                            <field name="NUM">1</field>
                                          </block>
                                        </value>
                                        <next>
                                          <block type="timeout">
                                            <statement name="TIMEOUTSTACK">
                                              <block type="trade_again"></block>
                                            </statement>
                                            <value name="SECONDS">
                                              <block type="math_number">
                                                <field name="NUM">0</field>
                                              </block>
                                            </value>
                                          </block>
                                        </next>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </statement>
        <statement name="ELSE">
          <block type="text_join">
            <field name="VARIABLE" id="text1">text1</field>
            <statement name="STACK">
              <block type="text_statement">
                <value name="TEXT">
                  <block type="text">
                    <field name="TEXT">Lost: </field>
                  </block>
                </value>
                <next>
                  <block type="text_statement">
                    <value name="TEXT">
                      <block type="math_single">
                        <field name="OP">ABS</field>
                        <value name="NUM">
                          <block type="read_details">
                            <field name="DETAIL_INDEX">4</field>
                          </block>
                        </value>
                      </block>
                    </value>
                  </block>
                </next>
              </block>
            </statement>
            <next>
              <block type="notify">
                <field name="NOTIFICATION_TYPE">warn</field>
                <field name="NOTIFICATION_SOUND">silent</field>
                <value name="MESSAGE">
                  <block type="variables_get">
                    <field name="VAR" id="text1">text1</field>
                  </block>
                </value>
                <next>
                  <block type="variables_set">
                    <field name="VAR" id="Time">Time</field>
                    <value name="VALUE">
                      <block type="math_number">
                        <field name="NUM">1</field>
                      </block>
                    </value>
                    <next>
                      <block type="variables_set">
                        <field name="VAR" id="Prediction">Prediction</field>
                        <value name="VALUE">
                          <block type="math_number">
                            <field name="NUM">5</field>
                          </block>
                        </value>
                        <next>
                          <block type="variables_set">
                            <field name="VAR" id="FibCurrent">FibCurrent</field>
                            <value name="VALUE">
                              <block type="math_arithmetic">
                                <field name="OP">ADD</field>
                                <value name="A">
                                  <block type="variables_get">
                                    <field name="VAR" id="FibPrev">FibPrev</field>
                                  </block>
                                </value>
                                <value name="B">
                                  <block type="variables_get">
                                    <field name="VAR" id="FibCurrent">FibCurrent</field>
                                  </block>
                                </value>
                              </block>
                            </value>
                            <next>
                              <block type="variables_set">
                                <field name="VAR" id="FibPrev">FibPrev</field>
                                <value name="VALUE">
                                  <block type="math_arithmetic">
                                    <field name="OP">MINUS</field>
                                    <value name="A">
                                      <block type="variables_get">
                                        <field name="VAR" id="FibCurrent">FibCurrent</field>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <block type="variables_get">
                                        <field name="VAR" id="FibPrev">FibPrev</field>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                                <next>
                                  <block type="variables_set">
                                    <field name="VAR" id="Initial_Amount">Initial Amount</field>
                                    <value name="VALUE">
                                      <block type="math_arithmetic">
                                        <field name="OP">MULTIPLY</field>
                                        <value name="A">
                                          <block type="variables_get">
                                            <field name="VAR" id="Win_Amount">Win Amount</field>
                                          </block>
                                        </value>
                                        <value name="B">
                                          <block type="variables_get">
                                            <field name="VAR" id="FibCurrent">FibCurrent</field>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                    <next>
                                      <block type="timeout">
                                        <statement name="TIMEOUTSTACK">
                                          <block type="trade_again"></block>
                                        </statement>
                                        <value name="SECONDS">
                                          <block type="math_number">
                                            <field name="NUM">1</field>
                                          </block>
                                        </value>
                                      </block>
                                    </next>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </statement>
        <next>
          <block type="text_join">
            <field name="VARIABLE" id="text2">text2</field>
            <statement name="STACK">
              <block type="text_statement">
                <value name="TEXT">
                  <block type="text">
                    <field name="TEXT">•Evans Eteng's FX Bot • Total Profit :$</field>
                  </block>
                </value>
                <next>
                  <block type="text_statement">
                    <value name="TEXT">
                      <block type="total_profit"></block>
                    </value>
                  </block>
                </next>
              </block>
            </statement>
            <next>
              <block type="notify">
                <field name="NOTIFICATION_TYPE">info</field>
                <field name="NOTIFICATION_SOUND">silent</field>
                <value name="MESSAGE">
                  <block type="variables_get">
                    <field name="VAR" id="text2">text2</field>
                  </block>
                </value>
                <next>
                  <block type="controls_if">
                    <mutation else="1"></mutation>
                    <value name="IF0">
                      <block type="logic_compare">
                        <field name="OP">LT</field>
                        <value name="A">
                          <block type="total_profit"></block>
                        </value>
                        <value name="B">
                          <block type="variables_get">
                            <field name="VAR" id="Expected_Profit">Expected Profit</field>
                          </block>
                        </value>
                      </block>
                    </value>
                    <statement name="DO0">
                      <block type="controls_if">
                        <mutation else="1"></mutation>
                        <value name="IF0">
                          <block type="logic_operation">
                            <field name="OP">AND</field>
                            <value name="A">
                              <block type="math_number_property">
                                <field name="PROPERTY">NEGATIVE</field>
                                <value name="NUMBER_TO_CHECK">
                                  <block type="total_profit"></block>
                                </value>
                              </block>
                            </value>
                            <value name="B">
                              <block type="logic_compare">
                                <field name="OP">GTE</field>
                                <value name="A">
                                  <block type="math_single">
                                    <field name="OP">ABS</field>
                                    <value name="NUM">
                                      <block type="total_profit"></block>
                                    </value>
                                  </block>
                                </value>
                                <value name="B">
                                  <block type="variables_get">
                                    <field name="VAR" id="Stop_Loss">Stop Loss</field>
                                  </block>
                                </value>
                              </block>
                            </value>
                          </block>
                        </value>
                        <statement name="DO0">
                          <block type="text_print">
                            <value name="TEXT">
                              <block type="text">
                                <field name="TEXT">Stop Loss</field>
                              </block>
                            </value>
                          </block>
                        </statement>
                        <statement name="ELSE">
                          <block type="text_join">
                            <field name="VARIABLE" id="text2">text2</field>
                            <statement name="STACK">
                              <block type="text_statement">
                                <value name="TEXT">
                                  <block type="text">
                                    <field name="TEXT">•Evans Eteng's FX Bot • Total Profit :$</field>
                                  </block>
                                </value>
                                <next>
                                  <block type="text_statement">
                                    <value name="TEXT">
                                      <block type="total_profit"></block>
                                    </value>
                                  </block>
                                </next>
                              </block>
                            </statement>
                            <next>
                              <block type="notify">
                                <field name="NOTIFICATION_TYPE">info</field>
                                <field name="NOTIFICATION_SOUND">silent</field>
                                <value name="MESSAGE">
                                  <block type="variables_get">
                                    <field name="VAR" id="text2">text2</field>
                                  </block>
                                </value>
                                <next>
                                  <block type="timeout">
                                    <statement name="TIMEOUTSTACK">
                                      <block type="trade_again"></block>
                                    </statement>
                                    <value name="SECONDS">
                                      <block type="math_number">
                                        <field name="NUM">1</field>
                                      </block>
                                    </value>
                                  </block>
                                </next>
                              </block>
                            </next>
                          </block>
                        </statement>
                      </block>
                    </statement>
                    <statement name="ELSE">
                      <block type="text_join">
                        <field name="VARIABLE" id="text3">text3</field>
                        <statement name="STACK">
                          <block type="text_statement">
                            <value name="TEXT">
                              <block type="text">
                                <field name="TEXT">•Evans Eteng's FX Bot • Total Profit :$</field>
                              </block>
                            </value>
                            <next>
                              <block type="text_statement">
                                <value name="TEXT">
                                  <block type="total_profit"></block>
                                </value>
                              </block>
                            </next>
                          </block>
                        </statement>
                        <next>
                          <block type="text_print">
                            <value name="TEXT">
                              <block type="variables_get">
                                <field name="VAR" id="text3">text3</field>
                              </block>
                            </value>
                          </block>
                        </next>
                      </block>
                    </statement>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>
  
  <block type="before_purchase" id="before_purchase">
    <statement name="BEFOREPURCHASE_STACK">
      <block type="variables_set">
        <field name="VAR" id="BB_Upper">BB_Upper</field>
        <value name="VALUE">
          <block type="bb">
            <field name="BB_RESULT">UPPER</field>
            <field name="PERIOD">20</field>
            <field name="STANDARD_DEVIATION">2</field>
            <field name="SOURCE">CLOSE</field>
          </block>
        </value>
        <next>
          <block type="variables_set">
            <field name="VAR" id="BB_Lower">BB_Lower</field>
            <value name="VALUE">
              <block type="bb">
                <field name="BB_RESULT">LOWER</field>
                <field name="PERIOD">20</field>
                <field name="STANDARD_DEVIATION">2</field>
                <field name="SOURCE">CLOSE</field>
              </block>
            </value>
            <next>
              <block type="variables_set">
                <field name="VAR" id="RSI">RSI</field>
                <value name="VALUE">
                  <block type="rsi">
                    <field name="PERIOD">14</field>
                    <field name="SOURCE">CLOSE</field>
                  </block>
                </value>
                <next>
                  <block type="controls_if">
                    <mutation else="1"></mutation>
                    <value name="IF0">
                      <block type="logic_operation">
                        <field name="OP">OR</field>
                        <value name="A">
                          <block type="logic_compare">
                            <field name="OP">LT</field>
                            <value name="A">
                              <block type="close"></block>
                            </value>
                            <value name="B">
                              <block type="variables_get">
                                <field name="VAR" id="BB_Lower">BB_Lower</field>
                              </block>
                            </value>
                          </block>
                        </value>
                        <value name="B">
                          <block type="logic_compare">
                            <field name="OP">LT</field>
                            <value name="A">
                              <block type="variables_get">
                                <field name="VAR" id="RSI">RSI</field>
                              </block>
                            </value>
                            <value name="B">
                              <block type="math_number">
                                <field name="NUM">30</field>
                              </block>
                            </value>
                          </block>
                        </value>
                      </block>
                    </value>
                    <statement name="DO0">
                      <block type="purchase">
                        <field name="PURCHASE_LIST">DIGITOVER</field>
                      </block>
                    </statement>
                    <statement name="ELSE">
                      <block type="controls_if">
                        <mutation else="1"></mutation>
                        <value name="IF0">
                          <block type="logic_operation">
                            <field name="OP">OR</field>
                            <value name="A">
                              <block type="logic_compare">
                                <field name="OP">GT</field>
                                <value name="A">
                                  <block type="close"></block>
                                </value>
                                <value name="B">
                                  <block type="variables_get">
                                    <field name="VAR" id="BB_Upper">BB_Upper</field>
                                  </block>
                                </value>
                              </block>
                            </value>
                            <value name="B">
                              <block type="logic_compare">
                                <field name="OP">GT</field>
                                <value name="A">
                                  <block type="variables_get">
                                    <field name="VAR" id="RSI">RSI</field>
                                  </block>
                                </value>
                                <value name="B">
                                  <block type="math_number">
                                    <field name="NUM">70</field>
                                  </block>
                                </value>
                              </block>
                            </value>
                          </block>
                        </value>
                        <statement name="DO0">
                          <block type="purchase">
                            <field name="PURCHASE_LIST">DIGITUNDER</field>
                          </block>
                        </statement>
                        <statement name="ELSE">
                          <block type="notify">
                            <field name="NOTIFICATION_TYPE">info</field>
                            <field name="NOTIFICATION_SOUND">silent</field>
                            <value name="MESSAGE">
                              <block type="text">
                                <field name="TEXT">No clear signal - skipping trade</field>
                              </block>
                            </value>
                          </block>
                        </statement>
                      </block>
                    </statement>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>
</xml>
